{
  "name": "tyloo-chat-server",
  "version": "2.1.7",
  "description": "",
  "author": "boboooooo159@gmail.com",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:type": "ts-node ./src/scripts/compile-joi.ts",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "pm2": "pm2 start --name tyloochat dist/main.js -i max",
    "pkg": "npm run build && ncc build dist/main.js -o deploy"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@nestjs/common": "7.6.14",
    "@nestjs/core": "7.6.14",
    "@nestjs/jwt": "7.2.0",
    "@nestjs/passport": "7.1.5",
    "@nestjs/platform-express": "7.6.14",
    "@nestjs/platform-fastify": "7.6.14",
    "@nestjs/platform-socket.io": "7.6.14",
    "@nestjs/swagger": "^4.8.0",
    "@nestjs/typeorm": "7.1.5",
    "@nestjs/websockets": "7.6.14",
    "@webundsoehne/nest-fastify-file-upload": "^1.0.0",
    "axios": "^0.21.0",
    "cache-manager": "^3.4.1",
    "cache-manager-redis-store": "^2.0.0",
    "crypto": "^1.0.1",
    "csurf": "^1.11.0",
    "extend": "^3.0.2",
    "fastify-compress": "^3.4.2",
    "fastify-helmet": "^5.3.1",
    "fastify-multer": "^2.0.2",
    "fastify-rate-limit": "^5.5.0",
    "fastify-static": "^4.0.1",
    "fastify-swagger": "^4.4.2",
    "humps": "^2.0.1",
    "joi-to-json-schema-support": "^5.2.0",
    "json-schema-to-ts": "^1.6.1",
    "lodash": "^4.17.21",
    "mongodb": "^3.5.9",
    "mssql": "^6.3.1",
    "mysql": "^2.18.1",
    "nest-winston": "^1.4.0",
    "node-rsa": "^1.1.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "request-promise": "^4.2.6",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "typeorm": "0.2.31",
    "uuid": "^8.2.0",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@types/express": "^4.17.3",
    "@types/extend": "^3.0.1",
    "@types/hapi__joi": "^17.1.6",
    "@types/humps": "^2.0.0",
    "@types/jest": "25.2.3",
    "@types/lodash": "^4.14.168",
    "@types/node": "^13.9.1",
    "@types/socket.io": "^2.1.8",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "3.0.2",
    "@typescript-eslint/parser": "3.0.2",
    "@zeit/ncc": "^0.22.3",
    "eslint": "7.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "jest": "26.0.1",
    "joi-to-typescript": "^1.11.0",
    "json-schema-to-typescript": "^10.1.3",
    "lint-staged": "^10.5.1",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "26.1.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
